{% extends 'layouts/layout.html.twig' %}

{% block content %}

    <h2>Home</h2>

    <button id="btn_subscribe" class="btn">Subscribe</button>

    <table class="table table-striped">
        <tr>
            <td>Titulo</td>
            <td>Descrição</td>
        </tr>    

        {% for post in posts %}
            <tr>
                <td>{{ post.title }}</td>
                <td>{{ post.description }}</td>
            </tr>    
        {% endfor %}
    </table>    

{% endblock %}